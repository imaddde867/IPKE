    {
      "id": "c1",
      "type": "condition",
      "text": "Clean daily in dusty or fire-hazard conditions; otherwise, clean at least weekly."
    },
    {
      "id": "c2",
      "type": "condition",
      "text": "Always use authentic replacement parts from known manufacturers."
    },
    {
      "id": "c3",
      "type": "condition",
      "text": "Shut down the engine before inspecting for fuel leaks."
    },
    {
      "id": "c4",
      "type": "condition",
      "text": "Cables in the engine compartment must have fastening brackets no more than 150 mm apart."
    },
    {
      "id": "c5",
      "type": "condition",
      "text": "Machines in hazardous environments (e.g., timber, peat) must have a fixed, automatic fire extinguishing system."
    },
    {
      "id": "c6",
      "type": "condition",
      "text": "Operators must be trained in the use of fire extinguishers."
    },
    {
      "id": "c7",
      "type": "condition",
      "text": "Ensure regeneration occurs in a safe outdoor environment away from flammable materials."
    },
    {
      "id": "c8",
      "type": "condition",
      "text": "Never leave the machine unattended during regeneration."
    },
    {
      "id": "c9",
      "type": "condition",
      "text": "If a fire occurs in a hazardous location, move the machine *before* shutting down the engine."
    },
    {
      "id": "c10",
      "type": "condition",
      "text": "Remain calm during an emergency."
    }

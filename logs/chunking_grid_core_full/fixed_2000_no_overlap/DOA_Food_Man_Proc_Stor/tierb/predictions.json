{
  "document_id": "DOA_Food_Man_Proc_Stor",
  "nodes": [
    {
      "id": "S1",
      "type": "step",
      "text": "Construct buildings with preventive design for pest management.",
      "order": 1
    },
    {
      "id": "S2",
      "type": "step",
      "text": "Maintain building to prevent pest entry.",
      "order": 2
    },
    {
      "id": "S3",
      "type": "step",
      "text": "Maintain daily pest management programs.",
      "order": 3
    },
    {
      "id": "S4",
      "type": "step",
      "text": "Address pests from facility and surrounding areas.",
      "order": 4
    },
    {
      "id": "S5",
      "type": "step",
      "text": "Follow federal and state regulations.",
      "order": 5
    },
    {
      "id": "S6",
      "type": "step",
      "text": "Design buildings for pest prevention.",
      "order": 6
    },
    {
      "id": "S7",
      "type": "step",
      "text": "Maintain short grass and trimmed shrubs.",
      "order": 7
    },
    {
      "id": "S8",
      "type": "step",
      "text": "Install paved access and proper drainage.",
      "order": 8
    },
    {
      "id": "S9",
      "type": "step",
      "text": "Create a gravel strip around the foundation.",
      "order": 9
    },
    {
      "id": "S10",
      "type": "step",
      "text": "Place night-lights at least 30 feet away from doors.",
      "order": 10
    },
    {
      "id": "S11",
      "type": "step",
      "text": "Keep framing at least four inches from walls.",
      "order": 11
    },
    {
      "id": "S12",
      "type": "step",
      "text": "Fill necessary voids with materials like polyurethane foam.",
      "order": 12
    },
    {
      "id": "S13",
      "type": "step",
      "text": "Use concrete, brick, or tile walls.",
      "order": 13
    },
    {
      "id": "S14",
      "type": "step",
      "text": "Seal and clean wall materials.",
      "order": 14
    },
    {
      "id": "S15",
      "type": "step",
      "text": "Avoid hollow panel-type walls or make them well-sealed.",
      "order": 15
    },
    {
      "id": "S16",
      "type": "step",
      "text": "Use smooth, free-standing water roofs.",
      "order": 16
    },
    {
      "id": "S17",
      "type": "step",
      "text": "Limit the number of windows and use transparent glass block.",
      "order": 17
    },
    {
      "id": "S18",
      "type": "step",
      "text": "Use metal, tight-fitting doors.",
      "order": 18
    },
    {
      "id": "S19",
      "type": "step",
      "text": "Install air curtains at large doors.",
      "order": 19
    },
    {
      "id": "S20",
      "type": "step",
      "text": "Use good lighting with dust-tight fixtures.",
      "order": 20
    },
    {
      "id": "S21",
      "type": "step",
      "text": "Install electrical and plumbing services for easy access and cleaning.",
      "order": 21
    },
    {
      "id": "S22",
      "type": "step",
      "text": "Make control panels insect-proof.",
      "order": 22
    },
    {
      "id": "S23",
      "type": "step",
      "text": "Equipment should be roomy, accessible for cleaning, and raised at least six inches off the floor.",
      "order": 23
    },
    {
      "id": "S24",
      "type": "step",
      "text": "Eliminate litter, waste, and uncut grass.",
      "order": 24
    },
    {
      "id": "S25",
      "type": "step",
      "text": "Pave dusty roads and maintain lawns.",
      "order": 25
    },
    {
      "id": "S26",
      "type": "step",
      "text": "Ensure proper drainage.",
      "order": 26
    },
    {
      "id": "S27",
      "type": "step",
      "text": "Position lighting away from buildings.",
      "order": 27
    },
    {
      "id": "S28",
      "type": "step",
      "text": "Reduce bird harborage and outside food spillage.",
      "order": 28
    },
    {
      "id": "S29",
      "type": "step",
      "text": "Use rodent, insect, and bird traps.",
      "order": 29
    },
    {
      "id": "S30",
      "type": "step",
      "text": "Maintain housekeeping and implement pest monitoring programs.",
      "order": 30
    },
    {
      "id": "S31",
      "type": "step",
      "text": "Use electrified grids inside for insect attraction.",
      "order": 31
    },
    {
      "id": "S32",
      "type": "step",
      "text": "Rodent-proof all openings.",
      "order": 32
    },
    {
      "id": "S33",
      "type": "step",
      "text": "Screen windows.",
      "order": 33
    },
    {
      "id": "S34",
      "type": "step",
      "text": "Block bird access to ledges.",
      "order": 34
    },
    {
      "id": "S35",
      "type": "step",
      "text": "Locate equipment off the floor or sealed to it.",
      "order": 35
    },
    {
      "id": "S36",
      "type": "step",
      "text": "Inspect all incoming vehicles and materials.",
      "order": 36
    },
    {
      "id": "S37",
      "type": "step",
      "text": "Use rodent traps near doors.",
      "order": 37
    },
    {
      "id": "S38",
      "type": "step",
      "text": "Ensure proper sanitation.",
      "order": 38
    },
    {
      "id": "S39",
      "type": "step",
      "text": "Use rodent bait stations where policy permits.",
      "order": 39
    },
    {
      "id": "S40",
      "type": "step",
      "text": "Use space treatments with nonresiduals, crack and crevice treatments with residuals, spot treatments, and fumigation for raw commodities.",
      "order": 40
    },
    {
      "id": "S41",
      "type": "step",
      "text": "Form an inspection committee and assign roles.",
      "order": 41
    },
    {
      "id": "S42",
      "type": "step",
      "text": "Conduct a thorough inspection of the plant.",
      "order": 42
    },
    {
      "id": "S43",
      "type": "step",
      "text": "Record all defects and distribute notes to department heads.",
      "order": 43
    },
    {
      "id": "S44",
      "type": "step",
      "text": "Departments correct identified defects.",
      "order": 44
    },
    {
      "id": "S45",
      "type": "step",
      "text": "Committee reviews notes, addresses uncorrected items, and assigns correction dates.",
      "order": 45
    },
    {
      "id": "S46",
      "type": "step",
      "text": "Sanitation is the responsibility of everyone in the plant.",
      "order": 46
    },
    {
      "id": "S47",
      "type": "step",
      "text": "Inspect places using a flashlight to detect insect trails or mouse footprints.",
      "order": 47
    },
    {
      "id": "S48",
      "type": "step",
      "text": "Observe suspect stock by spreading it on a flat surface and watching for movement.",
      "order": 48
    },
    {
      "id": "S49",
      "type": "step",
      "text": "Inspect areas most likely to produce results, such as under and inside equipment.",
      "order": 49
    },
    {
      "id": "S50",
      "type": "step",
      "text": "Suspect any unaccounted-for voids in equipment or structures.",
      "order": 50
    },
    {
      "id": "S51",
      "type": "step",
      "text": "Observe employee actions and in-plant practices.",
      "order": 51
    },
    {
      "id": "S52",
      "type": "step",
      "text": "Reward good sanitation to encourage plant-wide cooperation.",
      "order": 52
    },
    {
      "id": "S53",
      "type": "step",
      "text": "Consider periodic inspections by an outside firm for a fresh perspective.",
      "order": 53
    },
    {
      "id": "S54",
      "type": "step",
      "text": "Check temperature drops and their impact on mortality.",
      "order": 54
    },
    {
      "id": "S55",
      "type": "step",
      "text": "Implement good sanitation and temperature control.",
      "order": 55
    },
    {
      "id": "S56",
      "type": "step",
      "text": "Minimize insects' movement by inspecting incoming materials.",
      "order": 56
    },
    {
      "id": "S57",
      "type": "step",
      "text": "Design buildings to minimize entry points.",
      "order": 57
    },
    {
      "id": "S58",
      "type": "step",
      "text": "Use visual inspection for monitoring.",
      "order": 58
    },
    {
      "id": "S59",
      "type": "step",
      "text": "Consider using yellow or sodium arc lights outdoors.",
      "order": 59
    },
    {
      "id": "S60",
      "type": "step",
      "text": "Place traps based on season and temperature profiles.",
      "order": 60
    },
    {
      "id": "S61",
      "type": "step",
      "text": "Examine trends in trap catches over time.",
      "order": 61
    },
    {
      "id": "S62",
      "type": "step",
      "text": "Identify common insect pests in raw and processed food.",
      "order": 62
    },
    {
      "id": "S63",
      "type": "step",
      "text": "Select appropriate trap types based on environment.",
      "order": 63
    },
    {
      "id": "S64",
      "type": "step",
      "text": "Place traps on facility perimeter for outdoor pest control.",
      "order": 64
    },
    {
      "id": "S65",
      "type": "step",
      "text": "Determine trap placement and interpretation strategy.",
      "order": 65
    },
    {
      "id": "S66",
      "type": "step",
      "text": "Implement a pest management plan for the popcorn and seed industry.",
      "order": 66
    },
    {
      "id": "S67",
      "type": "step",
      "text": "Read the label carefully.",
      "order": 67
    },
    {
      "id": "S68",
      "type": "step",
      "text": "Identify food and non-food areas.",
      "order": 68
    },
    {
      "id": "S69",
      "type": "step",
      "text": "Understand types of residual applications: general, spot, and crack & crevice.",
      "order": 69
    },
    {
      "id": "S70",
      "type": "step",
      "text": "Check for pesticide regulations in your state.",
      "order": 70
    },
    {
      "id": "S71",
      "type": "step",
      "text": "Identify life cycle stages of beetles and moths: egg, larva, pupa, and adult.",
      "order": 71
    },
    {
      "id": "S72",
      "type": "step",
      "text": "Understand that beetle larvae do not have legs on the back half of their body.",
      "order": 72
    },
    {
      "id": "S73",
      "type": "step",
      "text": "Recognize that moth larvae have legs on the back half of their body.",
      "order": 73
    },
    {
      "id": "S74",
      "type": "step",
      "text": "Determine that adults of both species have chewing mouthparts and feed on food.",
      "order": 74
    },
    {
      "id": "S75",
      "type": "step",
      "text": "Identify moth species: Almond Moth, Indianmeal Moth, Mediterranean Flour Moth, Mealmoth.",
      "order": 75
    },
    {
      "id": "S76",
      "type": "step",
      "text": "Check for signs of larvae or webbing.",
      "order": 76
    },
    {
      "id": "S77",
      "type": "step",
      "text": "Inspect for beetle species: Lesser Grain Borer, Cigarette Beetle, Drugstore Beetle, Confused Flour Beetle, Red Flour Beetle, Sawtoothed Grain Beetle, Merchant Grain Beetle, Foreign Grain Beetle.",
      "order": 77
    },
    {
      "id": "S78",
      "type": "step",
      "text": "Apply control methods: Avoidance, sanitation, heat treatment, cold treatment, residual insecticides, space sprays, fumigation.",
      "order": 78
    },
    {
      "id": "S79",
      "type": "step",
      "text": "Identify pests: Spider Beetles, Warehouse Beetle / Dermestid Beetles, Cockroaches, Flies, Ants, Bees and Wasps, Booklice.",
      "order": 79
    },
    {
      "id": "S80",
      "type": "step",
      "text": "Determine pest behaviors and preferred environments.",
      "order": 80
    },
    {
      "id": "S81",
      "type": "step",
      "text": "Implement control measures based on pest behavior and environment.",
      "order": 81
    },
    {
      "id": "S82",
      "type": "step",
      "text": "Monitor for signs of infestation or re-infestation.",
      "order": 82
    },
    {
      "id": "S83",
      "type": "step",
      "text": "Identify locations with potential pest issues: silverfish, firebrats, crickets, ground beetles, sowbugs, and pillbugs.",
      "order": 83
    },
    {
      "id": "S84",
      "type": "step",
      "text": "Understand rodent behavior: senses, reactions, agility.",
      "order": 84
    },
    {
      "id": "S85",
      "type": "step",
      "text": "Recognize signs of rodents and other pests: droppings, runways, rubmarks, burrows, tracks, urine.",
      "order": 85
    },
    {
      "id": "S86",
      "type": "step",
      "text": "Implement sanitation and habitat modification strategies.",
      "order": 86
    },
    {
      "id": "S87",
      "type": "step",
      "text": "Rodent-proof construction: seal openings larger than 1/4 inch for mice and 1/2 inch for rats.",
      "order": 87
    },
    {
      "id": "S88",
      "type": "step",
      "text": "Use compressed air sprayer for crack and crevice, spot, and application.",
      "order": 88
    },
    {
      "id": "S89",
      "type": "step",
      "text": "Select appropriate nozzle for desired application.",
      "order": 89
    },
    {
      "id": "S90",
      "type": "step",
      "text": "Check nozzles and select appropriate type based on spray pattern.",
      "order": 90
    },
    {
      "id": "S91",
      "type": "step",
      "text": "Clean dusters after use.",
      "order": 91
    },
    {
      "id": "S92",
      "type": "step",
      "text": "Maintain space treating equipment by checking for proper function and selecting appropriate type based on application.",
      "order": 92
    },
    {
      "id": "S93",
      "type": "step",
      "text": "Wear appropriate respiratory protection as indicated on the label.",
      "order": 93
    },
    {
      "id": "S94",
      "type": "step",
      "text": "Obtain NIOSH approval number.",
      "order": 94
    },
    {
      "id": "S95",
      "type": "step",
      "text": "Ensure proper fit.",
      "order": 95
    },
    {
      "id": "S96",
      "type": "step",
      "text": "Perform daily pressure checks.",
      "order": 96
    },
    {
      "id": "S97",
      "type": "step",
      "text": "Avoid wearing beards.",
      "order": 97
    },
    {
      "id": "C1",
      "type": "condition",
      "text": "Thorough inspections must be conducted and all defects recorded."
    },
    {
      "id": "C2",
      "type": "condition",
      "text": "Require an 18-inch space between pallets and walls for inspection in warehouses."
    },
    {
      "id": "C3",
      "type": "condition",
      "text": "A written inspection procedure for incoming raw products is required by law."
    },
    {
      "id": "C4",
      "type": "condition",
      "text": "Monitor temperature drops to prevent high mortality."
    },
    {
      "id": "C5",
      "type": "condition",
      "text": "Minimize insects' movement to prevent infestations."
    },
    {
      "id": "C6",
      "type": "condition",
      "text": "Identify common insect pests in raw and processed food before implementing trapping strategies."
    },
    {
      "id": "C7",
      "type": "condition",
      "text": "Match trap types to the environment for effective pest control."
    },
    {
      "id": "C8",
      "type": "condition",
      "text": "Place traps on the facility perimeter to intercept and predict indoor infestations."
    },
    {
      "id": "C9",
      "type": "condition",
      "text": "Ensure pesticides are used according to their labels and applicable regulations."
    },
    {
      "id": "C10",
      "type": "condition",
      "text": "Ensure proper identification before applying pesticides to prevent harming beneficial insects or non-target organisms."
    },
    {
      "id": "C11",
      "type": "condition",
      "text": "Check for signs of infestation before consuming or selling food products."
    },
    {
      "id": "C12",
      "type": "condition",
      "text": "Properly store food products to prevent infestations."
    },
    {
      "id": "C13",
      "type": "condition",
      "text": "Ensure proper pest identification before implementing control measures."
    },
    {
      "id": "C14",
      "type": "condition",
      "text": "Maintain cleanliness and sanitation to prevent infestations."
    },
    {
      "id": "C15",
      "type": "condition",
      "text": "Effective pest control requires understanding the behavior and signs of various pests."
    },
    {
      "id": "C16",
      "type": "condition",
      "text": "Sanitation and habitat modification are essential for reducing rodent populations."
    },
    {
      "id": "C17",
      "type": "condition",
      "text": "Properly maintain equipment for safety and effectiveness."
    },
    {
      "id": "C18",
      "type": "condition",
      "text": "Wear appropriate respiratory protection when handling pesticides."
    }

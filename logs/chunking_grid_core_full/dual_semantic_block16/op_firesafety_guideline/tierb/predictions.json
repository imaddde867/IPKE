{
  "document_id": "op_firesafety_guideline",
  "nodes": [
    {
      "id": "S1",
      "type": "step",
      "text": "Identify potential risks in business operations.",
      "order": 1
    },
    {
      "id": "S2",
      "type": "step",
      "text": "Identify, analyze, assess, and process organizational risks.",
      "order": 2
    },
    {
      "id": "S3",
      "type": "step",
      "text": "Identify potential risks and their interconnections.",
      "order": 3
    },
    {
      "id": "S4",
      "type": "step",
      "text": "Perform business continuity planning.",
      "order": 4
    },
    {
      "id": "S5",
      "type": "step",
      "text": "Transfer risks through subcontracting or insurance.",
      "order": 5
    },
    {
      "id": "S6",
      "type": "step",
      "text": "Entrepreneur remains responsible according to insurance terms and safety regulations.",
      "order": 6
    },
    {
      "id": "S7",
      "type": "step",
      "text": "Company management is responsible for safety.",
      "order": 7
    },
    {
      "id": "S8",
      "type": "step",
      "text": "Issue clear instructions.",
      "order": 8
    },
    {
      "id": "S9",
      "type": "step",
      "text": "Supervise compliance.",
      "order": 9
    },
    {
      "id": "S10",
      "type": "step",
      "text": "Identify potential fire risks in working machines.",
      "order": 10
    },
    {
      "id": "S11",
      "type": "step",
      "text": "Evaluate each machine's operating environment.",
      "order": 11
    },
    {
      "id": "S12",
      "type": "step",
      "text": "Assess working environment.",
      "order": 12
    },
    {
      "id": "S13",
      "type": "step",
      "text": "Ensure conditions are normal.",
      "order": 13
    },
    {
      "id": "S14",
      "type": "step",
      "text": "Consider operating environment.",
      "order": 14
    },
    {
      "id": "S15",
      "type": "step",
      "text": "Determine if location is populated or remote.",
      "order": 15
    },
    {
      "id": "S16",
      "type": "step",
      "text": "Identify specific operating environment (sawmill, peat production area, etc.).",
      "order": 16
    },
    {
      "id": "S17",
      "type": "step",
      "text": "Consider the impact on the company's other risks.",
      "order": 17
    },
    {
      "id": "S18",
      "type": "step",
      "text": "Evaluate the level of risk the company is taking.",
      "order": 18
    },
    {
      "id": "S19",
      "type": "step",
      "text": "Assess employee actions in an accident.",
      "order": 19
    },
    {
      "id": "S20",
      "type": "step",
      "text": "Assess adequacy of preventive maintenance for business continuity.",
      "order": 20
    },
    {
      "id": "S21",
      "type": "step",
      "text": "Evaluate fire risk management in daily work.",
      "order": 21
    },
    {
      "id": "S22",
      "type": "step",
      "text": "Begin operation of a working machine.",
      "order": 22
    },
    {
      "id": "S23",
      "type": "step",
      "text": "Monitor machine for signs of overheating, leaks, or electrical issues.",
      "order": 23
    },
    {
      "id": "S24",
      "type": "step",
      "text": "Maintain high-quality service.",
      "order": 24
    },
    {
      "id": "S25",
      "type": "step",
      "text": "Use first-aid extinguishing equipment if a fire occurs.",
      "order": 25
    },
    {
      "id": "S26",
      "type": "step",
      "text": "Perform scheduled maintenance according to manufacturer's instructions.",
      "order": 26
    },
    {
      "id": "S27",
      "type": "step",
      "text": "Participate in manufacturer recall campaigns.",
      "order": 27
    },
    {
      "id": "S28",
      "type": "step",
      "text": "Use authentic replacement parts.",
      "order": 28
    },
    {
      "id": "S29",
      "type": "step",
      "text": "Obtain parts from known manufacturers.",
      "order": 29
    },
    {
      "id": "S30",
      "type": "step",
      "text": "Inspect and clean the engine compartment and surroundings daily in dusty or fire-hazard conditions.",
      "order": 30
    },
    {
      "id": "S31",
      "type": "step",
      "text": "Inspect and clean at least once a week in other environments.",
      "order": 31
    },
    {
      "id": "S32",
      "type": "step",
      "text": "Clean using compressed air.",
      "order": 32
    },
    {
      "id": "S33",
      "type": "step",
      "text": "Inspect for leaks.",
      "order": 33
    },
    {
      "id": "S34",
      "type": "step",
      "text": "Keep engine compartment clean.",
      "order": 34
    },
    {
      "id": "S35",
      "type": "step",
      "text": "Avoid exposure of flammable liquids to hot surfaces when temperature reaches about 240\u00b0C.",
      "order": 35
    },
    {
      "id": "S36",
      "type": "step",
      "text": "Check joints of hoses and pipes.",
      "order": 36
    },
    {
      "id": "S37",
      "type": "step",
      "text": "Inspect engine compartment for oil leaks.",
      "order": 37
    },
    {
      "id": "S38",
      "type": "step",
      "text": "Protect hydraulic hoses from high temperatures.",
      "order": 38
    },
    {
      "id": "S39",
      "type": "step",
      "text": "Use thermal shields or tubes for hoses near hot surfaces.",
      "order": 39
    },
    {
      "id": "S40",
      "type": "step",
      "text": "Repair any hydraulic hose leaks immediately.",
      "order": 40
    },
    {
      "id": "S41",
      "type": "step",
      "text": "Visually inspect electrical systems annually.",
      "order": 41
    },
    {
      "id": "S42",
      "type": "step",
      "text": "Document inspections.",
      "order": 42
    },
    {
      "id": "S43",
      "type": "step",
      "text": "Service electrical systems daily.",
      "order": 43
    },
    {
      "id": "S44",
      "type": "step",
      "text": "Inspect electrical systems.",
      "order": 44
    },
    {
      "id": "S45",
      "type": "step",
      "text": "Use a multimeter to measure voltage drops.",
      "order": 45
    },
    {
      "id": "S46",
      "type": "step",
      "text": "Check if drop exceeds 20W.",
      "order": 46
    },
    {
      "id": "S47",
      "type": "step",
      "text": "Use thermal imaging camera to detect anomalies in alternators, cables, and earthing points.",
      "order": 47
    },
    {
      "id": "S48",
      "type": "step",
      "text": "Compare readings with an intact machine.",
      "order": 48
    },
    {
      "id": "S49",
      "type": "step",
      "text": "Locate main power switch outside engine compartment.",
      "order": 49
    },
    {
      "id": "S50",
      "type": "step",
      "text": "Mark the main power switch visibly.",
      "order": 50
    },
    {
      "id": "S51",
      "type": "step",
      "text": "Check that power is cut off when the switch is used.",
      "order": 51
    },
    {
      "id": "S52",
      "type": "step",
      "text": "Install terminal covers on the switch poles.",
      "order": 52
    },
    {
      "id": "S53",
      "type": "step",
      "text": "Locate and operate the emergency kill switch before starting the machine.",
      "order": 53
    },
    {
      "id": "S54",
      "type": "step",
      "text": "Shuts down engine and cuts power.",
      "order": 54
    },
    {
      "id": "S55",
      "type": "step",
      "text": "Repair any malfunction.",
      "order": 55
    },
    {
      "id": "S56",
      "type": "step",
      "text": "Resume operation.",
      "order": 56
    },
    {
      "id": "S57",
      "type": "step",
      "text": "Repair any deviations immediately.",
      "order": 57
    },
    {
      "id": "S58",
      "type": "step",
      "text": "Ensure cable terminals are intact, firmly attached, and protected with terminal covers.",
      "order": 58
    },
    {
      "id": "S59",
      "type": "step",
      "text": "Provide sufficient ventilation for the battery compartment.",
      "order": 59
    },
    {
      "id": "S60",
      "type": "step",
      "text": "Check cables not protected by a fuse.",
      "order": 60
    },
    {
      "id": "S61",
      "type": "step",
      "text": "Inspect lead-throughs for rubber collars.",
      "order": 61
    },
    {
      "id": "S62",
      "type": "step",
      "text": "Replace cables with signs of abrasion or heat damage.",
      "order": 62
    },
    {
      "id": "S63",
      "type": "step",
      "text": "Protect them with a housing pipe.",
      "order": 63
    },
    {
      "id": "S64",
      "type": "step",
      "text": "Keep cables clean of oil and dirt.",
      "order": 64
    },
    {
      "id": "S65",
      "type": "step",
      "text": "Do not attach them to hoses or pipes.",
      "order": 65
    },
    {
      "id": "S66",
      "type": "step",
      "text": "Protect cable harnesses.",
      "order": 66
    },
    {
      "id": "S67",
      "type": "step",
      "text": "Firmly attach cable harnesses.",
      "order": 67
    },
    {
      "id": "S68",
      "type": "step",
      "text": "Make cable connections tight.",
      "order": 68
    },
    {
      "id": "S69",
      "type": "step",
      "text": "Install terminal covers on cable connections.",
      "order": 69
    },
    {
      "id": "S70",
      "type": "step",
      "text": "Check the electric panel during maintenance.",
      "order": 70
    },
    {
      "id": "S71",
      "type": "step",
      "text": "Repair any redundant or hanging wires.",
      "order": 71
    },
    {
      "id": "S72",
      "type": "step",
      "text": "Ensure fuse boxes are intact and have lids.",
      "order": 72
    },
    {
      "id": "S73",
      "type": "step",
      "text": "Check that fuses are the correct size.",
      "order": 73
    },
    {
      "id": "S74",
      "type": "step",
      "text": "Investigate the cause of any blown fuse.",
      "order": 74
    },
    {
      "id": "S75",
      "type": "step",
      "text": "Fasten cables with brackets no more than 150 mm apart.",
      "order": 75
    },
    {
      "id": "S76",
      "type": "step",
      "text": "Use fasteners and cable ties that can withstand temperatures of 120\u00b0C.",
      "order": 76
    },
    {
      "id": "S77",
      "type": "step",
      "text": "Do not attach cables to flammable material pipes or hoses.",
      "order": 77
    },
    {
      "id": "S78",
      "type": "step",
      "text": "Firmly attach alternator with correct belt tension.",
      "order": 78
    },
    {
      "id": "S79",
      "type": "step",
      "text": "Install terminal covers on alternator.",
      "order": 79
    },
    {
      "id": "S80",
      "type": "step",
      "text": "Keep alternator filter clean.",
      "order": 80
    },
    {
      "id": "S81",
      "type": "step",
      "text": "Check for anomalies with a thermal camera.",
      "order": 81
    },
    {
      "id": "S82",
      "type": "step",
      "text": "During maintenance, check fasteners.",
      "order": 82
    },
    {
      "id": "S83",
      "type": "step",
      "text": "Check belts.",
      "order": 83
    },
    {
      "id": "S84",
      "type": "step",
      "text": "Check bearings.",
      "order": 84
    },
    {
      "id": "S85",
      "type": "step",
      "text": "Replace noisy bearings.",
      "order": 85
    },
    {
      "id": "S86",
      "type": "step",
      "text": "Check exhaust pipe for leaks.",
      "order": 86
    },
    {
      "id": "S87",
      "type": "step",
      "text": "Ensure all thermal shields are in place.",
      "order": 87
    },
    {
      "id": "S88",
      "type": "step",
      "text": "Keep flammable liquid hoses away from each other.",
      "order": 88
    },
    {
      "id": "S89",
      "type": "step",
      "text": "Check Urea systems and diesel particulate filters.",
      "order": 89
    },
    {
      "id": "S90",
      "type": "step",
      "text": "Take care to prevent contamination during refuelling.",
      "order": 90
    },
    {
      "id": "S91",
      "type": "step",
      "text": "The diesel particulate filter is cleaned via regeneration.",
      "order": 91
    },
    {
      "id": "S92",
      "type": "step",
      "text": "Check machine indication for regeneration.",
      "order": 92
    },
    {
      "id": "S93",
      "type": "step",
      "text": "Perform regeneration in a safe outdoor environment.",
      "order": 93
    },
    {
      "id": "S94",
      "type": "step",
      "text": "Avoid flammable materials during regeneration.",
      "order": 94
    },
    {
      "id": "S95",
      "type": "step",
      "text": "Monitor regeneration process.",
      "order": 95
    },
    {
      "id": "S96",
      "type": "step",
      "text": "Check for fire hazards before starting the process.",
      "order": 96
    },
    {
      "id": "S97",
      "type": "step",
      "text": "Delay the process if machine is in a fire-hazard environment (e.g., underground, indoors).",
      "order": 97
    },
    {
      "id": "S98",
      "type": "step",
      "text": "Do not leave the machine unattended during regeneration.",
      "order": 98
    },
    {
      "id": "S99",
      "type": "step",
      "text": "Inspect all hoses and electrical systems in the swivelling joint.",
      "order": 99
    },
    {
      "id": "S100",
      "type": "step",
      "text": "Remove any rocks or twigs found.",
      "order": 100
    },
    {
      "id": "S101",
      "type": "step",
      "text": "Make sure all operating manuals are accessible in the machine.",
      "order": 101
    },
    {
      "id": "S102",
      "type": "step",
      "text": "Check that operating manuals are in a language the operator understands.",
      "order": 102
    },
    {
      "id": "S103",
      "type": "step",
      "text": "Check cleanliness and condition of auxiliary heater.",
      "order": 103
    },
    {
      "id": "S104",
      "type": "step",
      "text": "Examine heater compartment for fuel leaks.",
      "order": 104
    },
    {
      "id": "S105",
      "type": "step",
      "text": "Inspect the heater's exhaust pipe to ensure it doesn't touch anything flammable.",
      "order": 105
    },
    {
      "id": "S106",
      "type": "step",
      "text": "Test the heater under supervision before use.",
      "order": 106
    },
    {
      "id": "S107",
      "type": "step",
      "text": "Machines with a weight greater than 3,000 kg require at least two hand-held fire extinguishers.",
      "order": 107
    },
    {
      "id": "S108",
      "type": "step",
      "text": "Install frost-resistant liquid-based extinguishers with classification 34A233B.",
      "order": 108
    },
    {
      "id": "S109",
      "type": "step",
      "text": "Machines require four 34A233B extinguishers.",
      "order": 109
    },
    {
      "id": "S110",
      "type": "step",
      "text": "Extinguishers are to be made available at ground level.",
      "order": 110
    },
    {
      "id": "S111",
      "type": "step",
      "text": "Extinguishers must be easily accessible.",
      "order": 111
    },
    {
      "id": "S112",
      "type": "step",
      "text": "Extinguishers need clear marking.",
      "order": 112
    },
    {
      "id": "S113",
      "type": "step",
      "text": "Extinguishers require annual servicing.",
      "order": 113
    },
    {
      "id": "S114",
      "type": "step",
      "text": "Operators must be trained.",
      "order": 114
    },
    {
      "id": "S115",
      "type": "step",
      "text": "Autonomous machines must have a fixed fire extinguishing system.",
      "order": 115
    },
    {
      "id": "S116",
      "type": "step",
      "text": "Protect hot components, flammable liquid hoses/tanks, and electrical components.",
      "order": 116
    },
    {
      "id": "S117",
      "type": "step",
      "text": "Cover the entire engine compartment with detection system.",
      "order": 117
    },
    {
      "id": "S118",
      "type": "step",
      "text": "Protect hydraulic hoses in engine compartment with thermal protection tubes.",
      "order": 118
    },
    {
      "id": "S119",
      "type": "step",
      "text": "FFI has special requirements for fire extinguishing systems in working machines (Safety regulation FA-127).",
      "order": 119
    },
    {
      "id": "S120",
      "type": "step",
      "text": "Machines in dust or fire hazard environments (e.g., timber, peat processing) must have a fixed, fully automatic fire extinguishing system.",
      "order": 120
    },
    {
      "id": "S121",
      "type": "step",
      "text": "Ensure there is at least three litres of extinguishing agent for every cubic meter of engine or heater compartment.",
      "order": 121
    },
    {
      "id": "S122",
      "type": "step",
      "text": "Install detection system that covers the entire engine compartment and connect it to non-fused cables.",
      "order": 122
    },
    {
      "id": "S123",
      "type": "step",
      "text": "Install the certificate.",
      "order": 123
    },
    {
      "id": "S124",
      "type": "step",
      "text": "Obtain a test report for compliance.",
      "order": 124
    },
    {
      "id": "S125",
      "type": "step",
      "text": "Confirm system operation at start-up.",
      "order": 125
    },
    {
      "id": "S126",
      "type": "step",
      "text": "Repair any malfunctions.",
      "order": 126
    },
    {
      "id": "S127",
      "type": "step",
      "text": "Service the system at least once a year.",
      "order": 127
    },
    {
      "id": "S128",
      "type": "step",
      "text": "Clean engine compartments daily.",
      "order": 128
    },
    {
      "id": "S129",
      "type": "step",
      "text": "Mark fire extinguishing openings.",
      "order": 129
    },
    {
      "id": "S130",
      "type": "step",
      "text": "Choose approved extinguisher suppliers.",
      "order": 130
    },
    {
      "id": "S131",
      "type": "step",
      "text": "Inspect fire safety forms.",
      "order": 131
    },
    {
      "id": "S132",
      "type": "step",
      "text": "Check signs and markings.",
      "order": 132
    },
    {
      "id": "S133",
      "type": "step",
      "text": "Inspect machine electrical systems.",
      "order": 133
    },
    {
      "id": "S134",
      "type": "step",
      "text": "Examine engine mechanics.",
      "order": 134
    },
    {
      "id": "S135",
      "type": "step",
      "text": "Inspect first-aid extinguishing equipment.",
      "order": 135
    },
    {
      "id": "S136",
      "type": "step",
      "text": "Check auxiliary heater installation.",
      "order": 136
    },
    {
      "id": "S137",
      "type": "step",
      "text": "Inspect mains power systems.",
      "order": 137
    },
    {
      "id": "S138",
      "type": "step",
      "text": "Assess general condition and cleanliness.",
      "order": 138
    },
    {
      "id": "S139",
      "type": "step",
      "text": "Activate fire extinguisher in case of engine fire.",
      "order": 139
    },
    {
      "id": "S140",
      "type": "step",
      "text": "Remain calm.",
      "order": 140
    },
    {
      "id": "S141",
      "type": "step",
      "text": "Shut down the engine.",
      "order": 141
    },
    {
      "id": "S142",
      "type": "step",
      "text": "Move the machine indoors to outdoors.",
      "order": 142
    },
    {
      "id": "S143",
      "type": "step",
      "text": "Move the machine away from flammable material.",
      "order": 143
    },
    {
      "id": "S144",
      "type": "step",
      "text": "Cut electricity from the main power switch.",
      "order": 144
    },
    {
      "id": "S145",
      "type": "step",
      "text": "Turn off electricity.",
      "order": 145
    },
    {
      "id": "S146",
      "type": "step",
      "text": "Call for help.",
      "order": 146
    },
    {
      "id": "S147",
      "type": "step",
      "text": "Use the 112 smartphone app if possible.",
      "order": 147
    },
    {
      "id": "S148",
      "type": "step",
      "text": "Perform first-aid",
      "order": 148
    },
    {
      "id": "S149",
      "type": "step",
      "text": "Extinguish fire",
      "order": 149
    },
    {
      "id": "S150",
      "type": "step",
      "text": "Confirm that the fire is out with the fire department.",
      "order": 150
    },
    {
      "id": "S151",
      "type": "step",
      "text": "Previously, assumed the fire was controlled.",
      "order": 151
    },
    {
      "id": "S152",
      "type": "step",
      "text": "Report the incident to the safety officer, site manager, coworkers, and insurance company.",
      "order": 152
    },
    {
      "id": "S153",
      "type": "step",
      "text": "Report all accidents and dangerous situations to a supervisor immediately.",
      "order": 153
    },
    {
      "id": "S154",
      "type": "step",
      "text": "Stop the machine.",
      "order": 154
    },
    {
      "id": "S155",
      "type": "step",
      "text": "Leave the machine in place.",
      "order": 155
    },
    {
      "id": "S156",
      "type": "step",
      "text": "Wear protective clothing and a helmet.",
      "order": 156
    },
    {
      "id": "S157",
      "type": "step",
      "text": "Use a hands-free device for phone calls.",
      "order": 157
    },
    {
      "id": "S158",
      "type": "step",
      "text": "Wear a safety belt.",
      "order": 158
    },
    {
      "id": "S159",
      "type": "step",
      "text": "Use stepladders and handrails.",
      "order": 159
    },
    {
      "id": "S160",
      "type": "step",
      "text": "Maintain three points of contact when climbing.",
      "order": 160
    },
    {
      "id": "S161",
      "type": "step",
      "text": "Keep doors closed during operation.",
      "order": 161
    },
    {
      "id": "S162",
      "type": "step",
      "text": "Ensure tools are secure.",
      "order": 162
    },
    {
      "id": "S163",
      "type": "step",
      "text": "Mark emergency exits.",
      "order": 163
    },
    {
      "id": "S164",
      "type": "step",
      "text": "Repair any malfunctions that could lead to an accident without delay.",
      "order": 164
    },
    {
      "id": "S165",
      "type": "step",
      "text": "Arrange practical training on accident and fire procedures.",
      "order": 165
    },
    {
      "id": "S166",
      "type": "step",
      "text": "Check cables and fasteners for security, heat resistance, insulation, and lead-throughs.",
      "order": 166
    },
    {
      "id": "S167",
      "type": "step",
      "text": "Examine condition of fuses and batteries, including non-fused cables and terminal covers.",
      "order": 167
    },
    {
      "id": "S168",
      "type": "step",
      "text": "Clean engine and heater compartments.",
      "order": 168
    },
    {
      "id": "S169",
      "type": "step",
      "text": "Check for leaks in engine and heater compartments.",
      "order": 169
    },
    {
      "id": "S170",
      "type": "step",
      "text": "Ensure first-aid extinguishing equipment is available.",
      "order": 170
    },
    {
      "id": "S171",
      "type": "step",
      "text": "Verify markings on first-aid extinguishing equipment.",
      "order": 171
    },
    {
      "id": "S172",
      "type": "step",
      "text": "Provide operator training for using first-aid extinguishing equipment.",
      "order": 172
    },
    {
      "id": "C1",
      "type": "condition",
      "text": "Working in dangerous environments or with abnormal operating conditions increases risks."
    },
    {
      "id": "C2",
      "type": "condition",
      "text": "Be aware of potential hazards from toxic fumes."
    },
    {
      "id": "C3",
      "type": "condition",
      "text": "* A drop exceeding 20W indicates an increased fire risk."
    },
    {
      "id": "C4",
      "type": "condition",
      "text": "Working machines over 500 kg must have a main power switch located outside the engine compartment and visibly marked."
    },
    {
      "id": "C5",
      "type": "condition",
      "text": "Do not use for normal shutdown."
    },
    {
      "id": "C6",
      "type": "condition",
      "text": "Repair any deviations immediately to prevent overheating."
    },
    {
      "id": "C7",
      "type": "condition",
      "text": "Protect cable terminals with terminal covers to prevent overheating."
    },
    {
      "id": "C8",
      "type": "condition",
      "text": "Ensure alternator is securely attached with correct belt tension and terminal covers."
    },
    {
      "id": "C9",
      "type": "condition",
      "text": "Maintain a clean alternator filter to prevent overheating."
    },
    {
      "id": "C10",
      "type": "condition",
      "text": "* Urea systems and diesel particulate filter: Urea systems do not pose a fire risk, but leaks can cause corrosion and disrupt electrical equipment."
    },
    {
      "id": "C11",
      "type": "condition",
      "text": "Perform regeneration in a safe outdoor environment away from flammable materials."
    },
    {
      "id": "C12",
      "type": "condition",
      "text": "Machine should not be in a fire-hazard environment (underground, indoors) during the process."
    },
    {
      "id": "C13",
      "type": "condition",
      "text": "Do not leave machine unattended during regeneration."
    },
    {
      "id": "C14",
      "type": "condition",
      "text": "Ensure availability and comprehension of manuals for safe operation and malfunction response."
    },
    {
      "id": "C15",
      "type": "condition",
      "text": "Machines weighing over 3,000 kg must have at least two hand-held fire extinguishers."
    },
    {
      "id": "C16",
      "type": "condition",
      "text": "Frost-resistant liquid-based extinguishers with classification 34A233B are recommended."
    },
    {
      "id": "C17",
      "type": "condition",
      "text": "Extinguishers must be available at ground level, easily accessible, clearly marked, and serviced annually."
    },
    {
      "id": "C18",
      "type": "condition",
      "text": "Autonomous machines need six litres of agent per protected cubic metre."
    },
    {
      "id": "C19",
      "type": "condition",
      "text": "Machines in dust or fire hazard environments must have a fixed, fully automatic fire extinguishing system."
    },
    {
      "id": "C20",
      "type": "condition",
      "text": "Requirements for extinguishing agent and detection system."
    },
    {
      "id": "C21",
      "type": "condition",
      "text": "Engine compartment coverage and non-fused cable connection for detection system."
    },
    {
      "id": "C22",
      "type": "condition",
      "text": "Installation requires a certificate and compliance requires a test report."
    },
    {
      "id": "C23",
      "type": "condition",
      "text": "Repair any malfunctions immediately by an authorized shop."
    },
    {
      "id": "C24",
      "type": "condition",
      "text": "Check and address potential hazards before shutting down."
    }

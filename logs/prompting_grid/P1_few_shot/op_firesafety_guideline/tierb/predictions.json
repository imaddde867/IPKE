{
  "document_id": "op_firesafety_guideline",
  "nodes": [
    {
      "id": "S1",
      "type": "step",
      "text": "Determine employee actions in an accident",
      "order": 1
    },
    {
      "id": "S2",
      "type": "step",
      "text": "Evaluate preventive maintenance adequacy",
      "order": 2
    },
    {
      "id": "S3",
      "type": "step",
      "text": "Implement fire risk management during daily work",
      "order": 3
    },
    {
      "id": "S4",
      "type": "step",
      "text": "Perform scheduled maintenance according to manufacturer's instructions",
      "order": 4
    },
    {
      "id": "S5",
      "type": "step",
      "text": "Participate in manufacturer recall campaigns",
      "order": 5
    },
    {
      "id": "S6",
      "type": "step",
      "text": "Use authentic replacement parts from known manufacturers",
      "order": 6
    },
    {
      "id": "S7",
      "type": "step",
      "text": "Inspect and clean the engine compartment and surroundings",
      "order": 7
    },
    {
      "id": "S8",
      "type": "step",
      "text": "Use compressed air to remove dirt after cleaning",
      "order": 8
    },
    {
      "id": "S9",
      "type": "step",
      "text": "Inspect for leaks after cleaning",
      "order": 9
    },
    {
      "id": "S10",
      "type": "step",
      "text": "Check the engine compartment for cleanliness",
      "order": 10
    },
    {
      "id": "S11",
      "type": "step",
      "text": "Inspect for liquids and leaks in the engine compartment",
      "order": 11
    },
    {
      "id": "S12",
      "type": "step",
      "text": "Check joints of hoses and pipes",
      "order": 12
    },
    {
      "id": "S13",
      "type": "step",
      "text": "Check the engine compartment for oil leaks",
      "order": 13
    },
    {
      "id": "S14",
      "type": "step",
      "text": "Visually inspect electrical systems",
      "order": 14
    },
    {
      "id": "S15",
      "type": "step",
      "text": "Document the inspections",
      "order": 15
    },
    {
      "id": "S16",
      "type": "step",
      "text": "Use a multimeter to measure voltage drops",
      "order": 16
    },
    {
      "id": "S17",
      "type": "step",
      "text": "Inspect electrical systems daily on electric-powered and hybrid machines",
      "order": 17
    },
    {
      "id": "S18",
      "type": "step",
      "text": "Use a thermal imaging camera to detect anomalies in alternators, cables, and earthing points",
      "order": 18
    },
    {
      "id": "S19",
      "type": "step",
      "text": "Compare readings with an intact machine",
      "order": 19
    },
    {
      "id": "S20",
      "type": "step",
      "text": "Check main power switch",
      "order": 20
    },
    {
      "id": "S21",
      "type": "step",
      "text": "Shut down the engine and cut power",
      "order": 21
    },
    {
      "id": "S22",
      "type": "step",
      "text": "Repair any malfunction that required its use",
      "order": 22
    },
    {
      "id": "S23",
      "type": "step",
      "text": "Resume operation",
      "order": 23
    },
    {
      "id": "S24",
      "type": "step",
      "text": "Repair any deviations",
      "order": 24
    },
    {
      "id": "S25",
      "type": "step",
      "text": "Check cables not protected by a fuse (battery, alternator, starter motor)",
      "order": 25
    },
    {
      "id": "S26",
      "type": "step",
      "text": "Replace cables showing signs of abrasion or heat damage",
      "order": 26
    },
    {
      "id": "S27",
      "type": "step",
      "text": "Protect them with a housing pipe",
      "order": 27
    },
    {
      "id": "S28",
      "type": "step",
      "text": "Keep cables clean of oil and dirt",
      "order": 28
    },
    {
      "id": "S29",
      "type": "step",
      "text": "Do not attach cable harnesses to hoses or pipes",
      "order": 29
    },
    {
      "id": "S30",
      "type": "step",
      "text": "Protect and firmly attach cable harnesses",
      "order": 30
    },
    {
      "id": "S31",
      "type": "step",
      "text": "Ensure cable connections are tight and have terminal covers",
      "order": 31
    },
    {
      "id": "S32",
      "type": "step",
      "text": "Check the electric panel during maintenance",
      "order": 32
    },
    {
      "id": "S33",
      "type": "step",
      "text": "Repair any redundant or hanging wires",
      "order": 33
    },
    {
      "id": "S34",
      "type": "step",
      "text": "Ensure fuse boxes are intact and have lids",
      "order": 34
    },
    {
      "id": "S35",
      "type": "step",
      "text": "Check that fuses are the correct size",
      "order": 35
    },
    {
      "id": "S36",
      "type": "step",
      "text": "Investigate the cause of any blown fuse",
      "order": 36
    },
    {
      "id": "S37",
      "type": "step",
      "text": "Attach cables",
      "order": 37
    },
    {
      "id": "S38",
      "type": "step",
      "text": "The alternator must be firmly attached with correct belt tension and have terminal covers.",
      "order": 38
    },
    {
      "id": "S39",
      "type": "step",
      "text": "Keep any alternator filter clean to prevent overheating.",
      "order": 39
    },
    {
      "id": "S40",
      "type": "step",
      "text": "Check for anomalies with a thermal camera.",
      "order": 40
    },
    {
      "id": "S41",
      "type": "step",
      "text": "During maintenance, check fasteners, belts, and bearings.",
      "order": 41
    },
    {
      "id": "S42",
      "type": "step",
      "text": "Replace noisy bearings.",
      "order": 42
    },
    {
      "id": "S43",
      "type": "step",
      "text": "Exhaust pipe: Check for leaks and ensure all thermal shields are in place.",
      "order": 43
    },
    {
      "id": "S44",
      "type": "step",
      "text": "Never leave the machine unattended during regeneration",
      "order": 44
    },
    {
      "id": "S45",
      "type": "step",
      "text": "Inspect all hoses and electrical systems in the swivelling joint weekly",
      "order": 45
    },
    {
      "id": "S46",
      "type": "step",
      "text": "Remove any rocks or twigs immediately",
      "order": 46
    },
    {
      "id": "S47",
      "type": "step",
      "text": "Ensure all operating manuals are available in the machine in a language the operator understands",
      "order": 47
    },
    {
      "id": "S48",
      "type": "step",
      "text": "Before winter, check the cleanliness and condition of the auxiliary heater, its compartment, and its wiring",
      "order": 48
    },
    {
      "id": "S49",
      "type": "step",
      "text": "The heater compartment must be clean and free of fuel leaks",
      "order": 49
    },
    {
      "id": "S50",
      "type": "step",
      "text": "Inspect the heater's exhaust pipe to ensure it doesn't touch anything flammable",
      "order": 50
    },
    {
      "id": "S51",
      "type": "step",
      "text": "Test the heater under supervision before use",
      "order": 51
    },
    {
      "id": "S52",
      "type": "step",
      "text": "Confirm system's operation at start-up",
      "order": 52
    },
    {
      "id": "S53",
      "type": "step",
      "text": "Repair any malfunction immediately",
      "order": 53
    },
    {
      "id": "S54",
      "type": "step",
      "text": "Step 1: Prepare the surface by cleaning it with water and detergent",
      "order": 54
    },
    {
      "id": "S55",
      "type": "step",
      "text": "Step 2: Rinse the surface thoroughly with clean water",
      "order": 55
    },
    {
      "id": "S56",
      "type": "step",
      "text": "Step 3: Dry the surface with a lint-free cloth",
      "order": 56
    },
    {
      "id": "S57",
      "type": "step",
      "text": "Step 4: Apply the adhesive to the surface",
      "order": 57
    },
    {
      "id": "S58",
      "type": "step",
      "text": "Step 5: Press the patch onto the adhesive",
      "order": 58
    },
    {
      "id": "S59",
      "type": "step",
      "text": "Step 6: Allow the adhesive to cure for 24 hours",
      "order": 59
    },
    {
      "id": "S60",
      "type": "step",
      "text": "Call for help (emergency number 112)",
      "order": 60
    },
    {
      "id": "S61",
      "type": "step",
      "text": "Use the 112 smartphone app if possible",
      "order": 61
    },
    {
      "id": "S62",
      "type": "step",
      "text": "Perform first-aid fire extinguishing",
      "order": 62
    },
    {
      "id": "S63",
      "type": "step",
      "text": "Confirm fire is out with fire department",
      "order": 63
    },
    {
      "id": "S64",
      "type": "step",
      "text": "Report incident to safety officer, site manager, coworkers, and insurance company",
      "order": 64
    },
    {
      "id": "S65",
      "type": "step",
      "text": "Stop and leave machine in place if possible",
      "order": 65
    },
    {
      "id": "S66",
      "type": "step",
      "text": "Report all accidents and dangerous situations to a supervisor immediately",
      "order": 66
    },
    {
      "id": "S67",
      "type": "step",
      "text": "Wear protective clothing and a helmet",
      "order": 67
    },
    {
      "id": "S68",
      "type": "step",
      "text": "Use a hands-free device for phone calls",
      "order": 68
    },
    {
      "id": "S69",
      "type": "step",
      "text": "Wear a safety belt",
      "order": 69
    },
    {
      "id": "S70",
      "type": "step",
      "text": "Use stepladders and handrails",
      "order": 70
    },
    {
      "id": "S71",
      "type": "step",
      "text": "maintain three points of contact when climbing",
      "order": 71
    },
    {
      "id": "S72",
      "type": "step",
      "text": "Keep doors closed during operation",
      "order": 72
    },
    {
      "id": "S73",
      "type": "step",
      "text": "Ensure tools are secure",
      "order": 73
    },
    {
      "id": "S74",
      "type": "step",
      "text": "Ensure emergency exits are marked",
      "order": 74
    },
    {
      "id": "S75",
      "type": "step",
      "text": "Repair any malfunctions that could lead to an accident without delay",
      "order": 75
    },
    {
      "id": "C1",
      "type": "condition",
      "text": "Case-by-case basis"
    },
    {
      "id": "C2",
      "type": "condition",
      "text": "Considering operating environment"
    },
    {
      "id": "C3",
      "type": "condition",
      "text": "Personal injury poses a risk"
    },
    {
      "id": "C4",
      "type": "condition",
      "text": "Consider the impact on the company's other risks"
    },
    {
      "id": "C5",
      "type": "condition",
      "text": "Consider the level of risk the company is taking"
    },
    {
      "id": "C6",
      "type": "condition",
      "text": "Given: Accident scenario"
    },
    {
      "id": "C7",
      "type": "condition",
      "text": "Check: Preventive maintenance efficiency"
    },
    {
      "id": "C8",
      "type": "condition",
      "text": "Work environment: Daily operations"
    },
    {
      "id": "C9",
      "type": "condition",
      "text": "According to manufacturer's instructions"
    },
    {
      "id": "C10",
      "type": "condition",
      "text": "Authentic replacement parts from known manufacturers"
    },
    {
      "id": "C11",
      "type": "condition",
      "text": "Scheduled maintenance"
    },
    {
      "id": "C12",
      "type": "condition",
      "text": "Frequency: daily in dusty or fire-hazard conditions"
    },
    {
      "id": "C13",
      "type": "condition",
      "text": "Frequency: weekly in other environments"
    },
    {
      "id": "C14",
      "type": "condition",
      "text": "After a shift"
    },
    {
      "id": "C15",
      "type": "condition",
      "text": "In other environments"
    },
    {
      "id": "C16",
      "type": "condition",
      "text": "When operating in dusty or fire-hazard conditions (e.g., timber industry, peat processing)"
    },
    {
      "id": "C17",
      "type": "condition",
      "text": "Engine must be shut down before inspecting for fuel leaks"
    },
    {
      "id": "C18",
      "type": "condition",
      "text": "Hot surfaces reaching approximately 240\u00b0C increase fire risk for flammable liquids"
    },
    {
      "id": "C19",
      "type": "condition",
      "text": "Pressurized leaks can create flammable oil mists"
    },
    {
      "id": "C20",
      "type": "condition",
      "text": "At least once a year"
    },
    {
      "id": "C21",
      "type": "condition",
      "text": "Preferably during maintenance"
    },
    {
      "id": "C22",
      "type": "condition",
      "text": "Daily"
    },
    {
      "id": "C23",
      "type": "condition",
      "text": "Exceeding 20W indicates an increased fire risk"
    },
    {
      "id": "C24",
      "type": "condition",
      "text": "On electric-powered and hybrid machines"
    },
    {
      "id": "C25",
      "type": "condition",
      "text": "Compare readings with an intact machine"
    },
    {
      "id": "C26",
      "type": "condition",
      "text": "Working machines over 500 kg must have a main power switch located outside the engine compartment and visibly marked."
    },
    {
      "id": "C27",
      "type": "condition",
      "text": "* The switch poles must have terminal covers. Check that power is cut off when the switch is used."
    },
    {
      "id": "C28",
      "type": "condition",
      "text": "* For malfunction repairs only"
    },
    {
      "id": "C29",
      "type": "condition",
      "text": "Battery"
    },
    {
      "id": "C30",
      "type": "condition",
      "text": "Immediate repair"
    },
    {
      "id": "C31",
      "type": "condition",
      "text": "Intact, firmly attached, and protected with terminal covers"
    },
    {
      "id": "C32",
      "type": "condition",
      "text": "Sufficient ventilation"
    },
    {
      "id": "C33",
      "type": "condition",
      "text": "Frequency: several times a year"
    },
    {
      "id": "C34",
      "type": "condition",
      "text": "For lead-throughs, have rubber collars"
    },
    {
      "id": "C35",
      "type": "condition",
      "text": "Fuse boxes must be intact and have lids"
    },
    {
      "id": "C36",
      "type": "condition",
      "text": "Ensure fuses are the correct size"
    },
    {
      "id": "C37",
      "type": "condition",
      "text": "Cables in the high-vibration engine compartment must have fastening brackets no more than 150 mm apart"
    },
    {
      "id": "C38",
      "type": "condition",
      "text": "Fasteners and cable ties should withstand temperatures of 120\u00b0C"
    },
    {
      "id": "C39",
      "type": "condition",
      "text": "Do not attach cables to pipes or hoses containing flammable material"
    },
    {
      "id": "C40",
      "type": "condition",
      "text": "Never leave the machine unattended during regeneration"
    },
    {
      "id": "C41",
      "type": "condition",
      "text": "If the machine has a swivelling joint"
    },
    {
      "id": "C42",
      "type": "condition",
      "text": "Inspect all hoses and electrical systems in it weekly"
    },
    {
      "id": "C43",
      "type": "condition",
      "text": "Remove any rocks or twigs immediately"
    },
    {
      "id": "C44",
      "type": "condition",
      "text": "Ensure all operating manuals are available in the machine in a language the operator understands"
    },
    {
      "id": "C45",
      "type": "condition",
      "text": "Before winter"
    },
    {
      "id": "C46",
      "type": "condition",
      "text": "The heater compartment must be clean and free of fuel leaks"
    },
    {
      "id": "C47",
      "type": "condition",
      "text": "Under supervision"
    },
    {
      "id": "C48",
      "type": "condition",
      "text": "Machines > 3,000 kg must have"
    },
    {
      "id": "C49",
      "type": "condition",
      "text": "Machines for crushing timber or energy waste need"
    },
    {
      "id": "C50",
      "type": "condition",
      "text": "Extinguishers must be"
    },
    {
      "id": "C51",
      "type": "condition",
      "text": "Operators must be trained in their use."
    },
    {
      "id": "C52",
      "type": "condition",
      "text": "There must be at least three litres of extinguishing agent for each cubic meter of the engine or heater compartment"
    },
    {
      "id": "C53",
      "type": "condition",
      "text": "The detection system must cover the entire engine compartment and be connected to non-fused cables"
    },
    {
      "id": "C54",
      "type": "condition",
      "text": "Installation requires a certificate"
    },
    {
      "id": "C55",
      "type": "condition",
      "text": "Compliance requires a test report"
    },
    {
      "id": "C56",
      "type": "condition",
      "text": "Operation must be confirmed at start-up"
    }

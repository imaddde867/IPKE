{
  "document_id": "DOA_Food_Proc",
  "nodes": [
    {
      "id": "S1",
      "type": "step",
      "text": "Construct buildings with preventive design for pest management.",
      "order": 1
    },
    {
      "id": "S2",
      "type": "step",
      "text": "Maintain building to prevent pest entry.",
      "order": 2
    },
    {
      "id": "S3",
      "type": "step",
      "text": "Implement daily pest management programs.",
      "order": 3
    },
    {
      "id": "S4",
      "type": "step",
      "text": "Maintain records.",
      "order": 4
    },
    {
      "id": "S5",
      "type": "step",
      "text": "Address pests from facility and surrounding areas.",
      "order": 5
    },
    {
      "id": "S6",
      "type": "step",
      "text": "Follow federal and state regulations.",
      "order": 6
    },
    {
      "id": "S7",
      "type": "step",
      "text": "Design building for pest prevention.",
      "order": 7
    },
    {
      "id": "S8",
      "type": "step",
      "text": "Maintain short grass and trimmed shrubs.",
      "order": 8
    },
    {
      "id": "S9",
      "type": "step",
      "text": "Install paved access and proper drainage.",
      "order": 9
    },
    {
      "id": "S10",
      "type": "step",
      "text": "Create an 18- to 24-inch gravel strip around the foundation.",
      "order": 10
    },
    {
      "id": "S11",
      "type": "step",
      "text": "Place night-lights at least 30 feet away from doors.",
      "order": 11
    },
    {
      "id": "S12",
      "type": "step",
      "text": "Keep framing at least four inches from walls.",
      "order": 12
    },
    {
      "id": "S13",
      "type": "step",
      "text": "Fill necessary voids with materials like polyurethane foam.",
      "order": 13
    },
    {
      "id": "S14",
      "type": "step",
      "text": "Use concrete or brick walls that are sealed and easy to clean.",
      "order": 14
    },
    {
      "id": "S15",
      "type": "step",
      "text": "Seal hollow panel-type walls.",
      "order": 15
    },
    {
      "id": "S16",
      "type": "step",
      "text": "Use smooth, free-standing water roofs.",
      "order": 16
    },
    {
      "id": "S17",
      "type": "step",
      "text": "Limit the number of windows.",
      "order": 17
    },
    {
      "id": "S18",
      "type": "step",
      "text": "Use transparent glass block for windows.",
      "order": 18
    },
    {
      "id": "S19",
      "type": "step",
      "text": "Install metal and tight-fitting doors.",
      "order": 19
    },
    {
      "id": "S20",
      "type": "step",
      "text": "Use air curtains at large doors.",
      "order": 20
    },
    {
      "id": "S21",
      "type": "step",
      "text": "Install good lighting with dust-tight fixtures.",
      "order": 21
    },
    {
      "id": "S22",
      "type": "step",
      "text": "Install electrical and plumbing services for easy access and cleaning.",
      "order": 22
    },
    {
      "id": "S23",
      "type": "step",
      "text": "Make control panels insect-proof.",
      "order": 23
    },
    {
      "id": "S24",
      "type": "step",
      "text": "Eliminate litter, waste, and uncut grass.",
      "order": 24
    },
    {
      "id": "S25",
      "type": "step",
      "text": "Pave dusty roads and maintain lawns.",
      "order": 25
    },
    {
      "id": "S26",
      "type": "step",
      "text": "Ensure proper drainage.",
      "order": 26
    },
    {
      "id": "S27",
      "type": "step",
      "text": "Position lighting away from buildings.",
      "order": 27
    },
    {
      "id": "S28",
      "type": "step",
      "text": "Reduce bird harborage and outside food spillage.",
      "order": 28
    },
    {
      "id": "S29",
      "type": "step",
      "text": "Use rodent, insect, and bird traps.",
      "order": 29
    },
    {
      "id": "S30",
      "type": "step",
      "text": "Maintain housekeeping and implement pest monitoring programs.",
      "order": 30
    },
    {
      "id": "S31",
      "type": "step",
      "text": "Maintain bait stations for rodent management.",
      "order": 31
    },
    {
      "id": "S32",
      "type": "step",
      "text": "Use insecticidal baits and approved insecticides near breeding sites.",
      "order": 32
    },
    {
      "id": "S33",
      "type": "step",
      "text": "Use chemical repellents and avicides where appropriate for bird management.",
      "order": 33
    },
    {
      "id": "S34",
      "type": "step",
      "text": "Rodent-proof all openings.",
      "order": 34
    },
    {
      "id": "S35",
      "type": "step",
      "text": "Screen windows.",
      "order": 35
    },
    {
      "id": "S36",
      "type": "step",
      "text": "Block bird access to ledges.",
      "order": 36
    },
    {
      "id": "S37",
      "type": "step",
      "text": "Locate equipment off the floor or sealed to it.",
      "order": 37
    },
    {
      "id": "S38",
      "type": "step",
      "text": "Inspect all incoming vehicles and materials.",
      "order": 38
    },
    {
      "id": "S39",
      "type": "step",
      "text": "Store materials away from walls for inspection access.",
      "order": 39
    },
    {
      "id": "S40",
      "type": "step",
      "text": "Use rodent traps near doors and map their locations.",
      "order": 40
    },
    {
      "id": "S41",
      "type": "step",
      "text": "Use air curtains at doors.",
      "order": 41
    },
    {
      "id": "S42",
      "type": "step",
      "text": "Use deterrents for birds on roosting sites.",
      "order": 42
    },
    {
      "id": "S43",
      "type": "step",
      "text": "Implement a detailed monitoring program and a total facility IPM program.",
      "order": 43
    },
    {
      "id": "S44",
      "type": "step",
      "text": "Use rodent traps near doors if needed.",
      "order": 44
    },
    {
      "id": "S45",
      "type": "step",
      "text": "Ensure proper sanitation.",
      "order": 45
    },
    {
      "id": "S46",
      "type": "step",
      "text": "Use space treatments with nonresiduals, crack and crevice treatments with residuals, spot treatments, and fumigation for raw commodities.",
      "order": 46
    },
    {
      "id": "S47",
      "type": "step",
      "text": "Form an inspection committee and assign roles.",
      "order": 47
    },
    {
      "id": "S48",
      "type": "step",
      "text": "Conduct a thorough inspection of the plant.",
      "order": 48
    },
    {
      "id": "S49",
      "type": "step",
      "text": "Record all defects and distribute notes to department heads.",
      "order": 49
    },
    {
      "id": "S50",
      "type": "step",
      "text": "Departments correct identified defects.",
      "order": 50
    },
    {
      "id": "S51",
      "type": "step",
      "text": "Committee reviews notes, addresses uncorrected items, and assigns correction dates.",
      "order": 51
    },
    {
      "id": "S52",
      "type": "step",
      "text": "Sanitation is the responsibility of everyone in the plant.",
      "order": 52
    },
    {
      "id": "S53",
      "type": "step",
      "text": "Inspect areas using a flashlight to detect insect trails or mouse footprints.",
      "order": 53
    },
    {
      "id": "S54",
      "type": "step",
      "text": "Observe suspect stock by spreading it on a flat surface and watching for movement.",
      "order": 54
    },
    {
      "id": "S55",
      "type": "step",
      "text": "Inspect areas most likely to produce results, such as under and inside equipment.",
      "order": 55
    },
    {
      "id": "S56",
      "type": "step",
      "text": "Suspect any unaccounted-for voids in equipment or structures.",
      "order": 56
    },
    {
      "id": "S57",
      "type": "step",
      "text": "Observe employee actions and in-plant practices.",
      "order": 57
    },
    {
      "id": "S58",
      "type": "step",
      "text": "Reward good sanitation to encourage plant-wide cooperation.",
      "order": 58
    },
    {
      "id": "S59",
      "type": "step",
      "text": "Consider periodic inspections by an outside firm for a fresh perspective.",
      "order": 59
    },
    {
      "id": "S60",
      "type": "step",
      "text": "Check temperature drop can cause high mortality.",
      "order": 60
    },
    {
      "id": "S61",
      "type": "step",
      "text": "Implement good sanitation and temperature control.",
      "order": 61
    },
    {
      "id": "S62",
      "type": "step",
      "text": "Minimize insect movement by inspecting incoming materials.",
      "order": 62
    },
    {
      "id": "S63",
      "type": "step",
      "text": "Design buildings to minimize entry points.",
      "order": 63
    },
    {
      "id": "S64",
      "type": "step",
      "text": "Use visual inspection for monitoring.",
      "order": 64
    },
    {
      "id": "S65",
      "type": "step",
      "text": "Consider using yellow or sodium arc lights outdoors.",
      "order": 65
    },
    {
      "id": "S66",
      "type": "step",
      "text": "Place pheromone-baited traps in high-risk areas.",
      "order": 66
    },
    {
      "id": "S67",
      "type": "step",
      "text": "Examine trends in trap catches over time.",
      "order": 67
    },
    {
      "id": "S68",
      "type": "step",
      "text": "Identify common insect pests in raw grain and processed food.",
      "order": 68
    },
    {
      "id": "S69",
      "type": "step",
      "text": "Select appropriate trap types based on the environment.",
      "order": 69
    },
    {
      "id": "S70",
      "type": "step",
      "text": "Place traps on the outer perimeter of a facility for interception and prediction.",
      "order": 70
    },
    {
      "id": "S71",
      "type": "step",
      "text": "Monitor trap catches for increases in numbers from one period to the next.",
      "order": 71
    },
    {
      "id": "S72",
      "type": "step",
      "text": "Implement a pest management plan, including monitoring and inspection, building modifications, non-chemical control, and chemical control.",
      "order": 72
    },
    {
      "id": "S73",
      "type": "step",
      "text": "Read the label carefully before using any pesticide.",
      "order": 73
    },
    {
      "id": "S74",
      "type": "step",
      "text": "Understand food-handling establishment areas: food, food processing facilities, nonfood areas, and food areas.",
      "order": 74
    },
    {
      "id": "S75",
      "type": "step",
      "text": "Apply pesticides according to their labels: general applications (nonfood areas), spot applications (limited areas), or crack and crevice applications (food areas).",
      "order": 75
    },
    {
      "id": "S76",
      "type": "step",
      "text": "Follow pesticide regulations at the federal and state levels.",
      "order": 76
    },
    {
      "id": "S77",
      "type": "step",
      "text": "Identify life cycle stages of beetles and moths: egg, larva, pupa, and adult.",
      "order": 77
    },
    {
      "id": "S78",
      "type": "step",
      "text": "Understand that beetle larvae do not have legs on the back half of their body.",
      "order": 78
    },
    {
      "id": "S79",
      "type": "step",
      "text": "Recognize that adult beetles have chewing mouthparts and feed on various food sources.",
      "order": 79
    },
    {
      "id": "S80",
      "type": "step",
      "text": "Identify moth species: Almond Moth, Indianmeal Moth, Mediterranean Flour Moth, Mealmoth.",
      "order": 80
    },
    {
      "id": "S81",
      "type": "step",
      "text": "Check for signs of moth infestation in food products.",
      "order": 81
    },
    {
      "id": "S82",
      "type": "step",
      "text": "Identify beetle species: Lesser Grain Borer, Cigarette Beetle, Drugstore Beetle, Confused Flour Beetle, Sawtoothed Grain Beetle, Merchant Grain Beetle, Foreign Grain Beetle, Rusty Grain Beetle.",
      "order": 82
    },
    {
      "id": "S83",
      "type": "step",
      "text": "Inspect food products for signs of beetle infestation.",
      "order": 83
    },
    {
      "id": "S84",
      "type": "step",
      "text": "Implement control measures: Avoidance, sanitation, heat treatment, cold treatment, residual insecticides, space sprays, fumigation.",
      "order": 84
    },
    {
      "id": "S85",
      "type": "step",
      "text": "Identify pests: Spider Beetles, Warehouse Beetle / Dermestid Beetles, Cockroaches, Flies, Ants, Bees and Wasps, Booklice.",
      "order": 85
    },
    {
      "id": "S86",
      "type": "step",
      "text": "Determine pest characteristics: appearance, behavior, preferred habitats, signs of infestation.",
      "order": 86
    },
    {
      "id": "S87",
      "type": "step",
      "text": "Implement control measures based on pest type: sanitation, residual pesticides, dusts, contact sprays, nest treatment.",
      "order": 87
    },
    {
      "id": "S88",
      "type": "step",
      "text": "Monitor and evaluate effectiveness of control measures.",
      "order": 88
    },
    {
      "id": "S89",
      "type": "step",
      "text": "Identify pest species: Silverfish, Firebrats, Crickets, Ground Beetles, Sowbugs and Pillbugs, Rats, or Mice.",
      "order": 89
    },
    {
      "id": "S90",
      "type": "step",
      "text": "Understand pest behavior: senses, reactions, agility.",
      "order": 90
    },
    {
      "id": "S91",
      "type": "step",
      "text": "Recognize signs: droppings, runways, rubmarks, burrows, tracks, urine.",
      "order": 91
    },
    {
      "id": "S92",
      "type": "step",
      "text": "Implement rodent management: sanitation and habitat modification, rodent-proof construction.",
      "order": 92
    },
    {
      "id": "S93",
      "type": "step",
      "text": "Use compressed air sprayer for crack and crevice, spot, and general sprays.",
      "order": 93
    },
    {
      "id": "S94",
      "type": "step",
      "text": "Fill the tank with appropriate pesticide.",
      "order": 94
    },
    {
      "id": "S95",
      "type": "step",
      "text": "Clean hose, valve, and nozzle after each use.",
      "order": 95
    },
    {
      "id": "S96",
      "type": "step",
      "text": "Check gaskets and valves regularly.",
      "order": 96
    },
    {
      "id": "S97",
      "type": "step",
      "text": "Never leave pressurized equipment unattended.",
      "order": 97
    },
    {
      "id": "S98",
      "type": "step",
      "text": "Choose appropriate nozzle for spray pattern.",
      "order": 98
    },
    {
      "id": "S99",
      "type": "step",
      "text": "Maintain dusters by keeping them clean and dry.",
      "order": 99
    },
    {
      "id": "S100",
      "type": "step",
      "text": "Follow instructions for using space treating equipment.",
      "order": 100
    },
    {
      "id": "S101",
      "type": "step",
      "text": "Wear appropriate respiratory protection as needed.",
      "order": 101
    },
    {
      "id": "S102",
      "type": "step",
      "text": "Perform daily negative and positive pressure checks.",
      "order": 102
    },
    {
      "id": "S103",
      "type": "step",
      "text": "Wash respirators regularly.",
      "order": 103
    },
    {
      "id": "S104",
      "type": "step",
      "text": "Store respirators in a clean, protected place.",
      "order": 104
    },
    {
      "id": "C1",
      "type": "condition",
      "text": "Use IPM approach integrating chemical, physical, cultural, and biological controls."
    },
    {
      "id": "C2",
      "type": "condition",
      "text": "Thorough inspections must be conducted in all areas of the plant."
    },
    {
      "id": "C3",
      "type": "condition",
      "text": "Require an 18-inch space between pallets and walls for inspection in warehouses."
    },
    {
      "id": "C4",
      "type": "condition",
      "text": "A written inspection procedure for incoming raw products is required by law."
    },
    {
      "id": "C5",
      "type": "condition",
      "text": "Implement temperature control and good sanitation to slow pest growth."
    },
    {
      "id": "C6",
      "type": "condition",
      "text": "Minimize insect movement by inspecting incoming materials and designing buildings."
    },
    {
      "id": "C7",
      "type": "condition",
      "text": "Match trap types to the environment (dusty vs. clean, hot vs. cold)."
    },
    {
      "id": "C8",
      "type": "condition",
      "text": "Detect pests and adjust trap placement or add more traps."
    },
    {
      "id": "C9",
      "type": "condition",
      "text": "Ensure that pesticides are used in accordance with their labels to maintain regulatory compliance."
    },
    {
      "id": "C10",
      "type": "condition",
      "text": "Ensure proper application of pesticides only to approved sites as per label instructions."
    },
    {
      "id": "C11",
      "type": "condition",
      "text": "Maintain a safe working environment by considering potential interactions with facility practices before application."
    },
    {
      "id": "C12",
      "type": "condition",
      "text": "Check for signs of moth or beetle infestation before consuming food products."
    },
    {
      "id": "C13",
      "type": "condition",
      "text": "Store food products in airtight containers to prevent infestations."
    },
    {
      "id": "C14",
      "type": "condition",
      "text": "Ensure proper identification of pests before implementing control measures."
    },
    {
      "id": "C15",
      "type": "condition",
      "text": "Follow safety guidelines when handling pesticides and treating nests."
    },
    {
      "id": "C16",
      "type": "condition",
      "text": "Understand pest behavior before implementing control measures."
    },
    {
      "id": "C17",
      "type": "condition",
      "text": "Maintain clean facilities to minimize rodent presence."
    },
    {
      "id": "C18",
      "type": "condition",
      "text": "Ensure equipment is safe and effective by following maintenance procedures."
    },
    {
      "id": "C19",
      "type": "condition",
      "text": "Use appropriate nozzle for desired spray pattern."
    },
    {
      "id": "C20",
      "type": "condition",
      "text": "Follow safety guidelines when using space treating equipment."
    },
    {
      "id": "C21",
      "type": "condition",
      "text": "Wear appropriate respiratory protection as indicated on the label."
    }
